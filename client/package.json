  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/airbnb",
      "@vue/typescript"
    ],
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    },
    "rules": {
      "no-mixed-operators": 0,
      "no-param-reassign": 0,
      "no-plusplus": 0,
      "no-shadow": 0,
      "no-continue": 0,
      "keyword-spacing": 1,
      "semi": 1,
      "padded-blocks": 1,
      "no-useless-constructor": 0,
      "no-empty-function": 0,
      "no-unused-vars": 0,
      "max-len": 0,
      "no-nested-ternary": 0
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
