{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Client Test File",
      "autoAttachChildProcesses": true,
      "skipFiles": [],
      "program": "${workspaceRoot}/client/node_modules/vitest/vitest.mjs",
      "args": ["--config", "client/vitest.config.ts", "run", "${relativeFile}", "-u"],
      "smartStep": true,
      "console": "integratedTerminal"
    }
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Server Test File",
      "autoAttachChildProcesses": true,
      "skipFiles": [],
      "program": "${workspaceRoot}/server/node_modules/vitest/vitest.mjs",
      "args": ["--config", "server/vitest.config.ts", "run", "${relativeFile}", "-u"],
      "smartStep": true,
      "console": "integratedTerminal"
    }
  ]
}